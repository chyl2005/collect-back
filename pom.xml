        <!-- freemarker版本 -->
        <freemarker.version>2.3.20</freemarker.version>
        <!-- jackson版本 -->
        <jackson.version>2.5.1</jackson.version>
        <!-- jsoup版本 -->
        <jsoup.version>1.8.1</jsoup.version>
        <jstl.version>1.2.1</jstl.version>
