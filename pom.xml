        <!--项目版本 -->
        <project.version>1.0.0-SNAPSHOT</project.version>
        <!-- junit版本 -->
        <junit.version>4.12</junit.version>
        <!--spring版本 -->
        <spring.version>4.3.5.RELEASE</spring.version>
        <!--hibernate版本 -->
